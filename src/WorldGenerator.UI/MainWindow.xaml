<Window x:Class="WorldGenerator.UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WorldGenerator.UI"
        mc:Ignorable="d"
        Title="MainWindow" ResizeMode="NoResize" SizeToContent="WidthAndHeight">
    <Grid>
        <StackPanel>
            <Button Click="GenerateClicked">Generate</Button>
            <Canvas x:Name="canvas" Background="White" Width="500" Height="500"></Canvas>
        </StackPanel>
            

    </Grid>
</Window>
